{
  "name": "electric-shop-backend",
  "version": "1.0.0",
  "description": "Backend for Electric Shop E-Commerce",
  "main": "server.js",
  "scripts": {
    "start": "node --openssl-legacy-provider server.js",
    "dev": "node validate-startup.js && nodemon --exec node --openssl-legacy-provider server.js",
    "seed": "node --openssl-legacy-provider seeder.js",
    "seed:catalog": "node --openssl-legacy-provider seed-catalog.js",
    "seed:catalog:reset": "node --openssl-legacy-provider seed-catalog.js -d",
    "seed:services": "node --openssl-legacy-provider seed-services.js",
    "seed:pipes": "node --openssl-legacy-provider seed-pipes.js",
    "validate": "node validate-startup.js",
    "test-db": "node check-mongodb.js"
  },
  "keywords": [
    "ecommerce",
    "electric-shop",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.11",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
